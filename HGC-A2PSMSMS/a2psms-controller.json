{
   "kind":"ReplicationController",
   "apiVersion":"v1",
   "metadata":{
      "name":"a2psms",
      "labels":{
         "app":"a2psms"
      }
   },
   "spec":{
      "replicas":3,
      "selector":{
         "app":"a2psms"
      },
      "template":{
         "metadata":{
            "labels":{
               "app":"a2psms"
            }
         },
         "spec":{
            "containers":[
               {
                  "name":"a2psms-container",
                  "image":"mimilaw123/a2psms:20181024",
                  "ports":[
                     {
                        "name":"a2psms-port",
                        "containerPort":8080
                     }
                  ]
               }
            ]
         }
      }
   }
}
